{
    "name": "reg",
    "displayName": "REG",
    "description": "Windows Registry Script (.reg) Language package for VSCode",
    "version": "1.0.3",
    "publisher": "ionutvmi",
    "icon": "icon.png",
    "repository": {
        "type": "git",
        "url": "https://github.com/ionutvmi/reg-vscode"
    },
    "bugs": {
        "url": "https://github.com/ionutvmi/reg-vscode/issues",
        "email": "ionutvmi@gmail.com"
    },
    "engines": {
        "vscode": "^1.0.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "reg",
                "aliases": [
                    "REG",
                    "reg"
                ],
                "extensions": [
                    ".reg",
                    ".REG"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "reg",
                "scopeName": "source.reg",
                "path": "./syntaxes/reg.tmLanguage"
            }
        ],
        "commands": [
            {
                "command": "extension.reg.jumpToKey",
                "title": "REG: Jump to registry key"
            }
        ],
        "snippets": [
            {
                "language": "reg",
                "path": "./snippets/snippets.json"
            }
        ]
    },
    "activationEvents": [
        "onLanguage:reg"
    ],
    "main": "./out/extension",
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./"
    },
    "devDependencies": {
        "typescript": "4.6.3",
        "@types/vscode": "1.66.0",
        "tslint": "6.1.3",
        "@types/node": "17.0.24"
    },
    "dependencies": {
        "sudo-prompt": "9.2.1"
    }
}
